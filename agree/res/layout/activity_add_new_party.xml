<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AddNewPartyActivity" >

    <RelativeLayout
        android:id="@+id/Add_New_Party_title_layout"
        android:layout_width="match_parent"
        android:layout_height="50.0dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" >

        <RelativeLayout
            android:id="@+id/Add_New_Party_back_layout"
            android:layout_width="@dimen/title_left_width"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true" >

            <TextView
                android:id="@+id/Add_New_Party_back"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:text="@string/newparty_title_left"
                android:textColor="@drawable/title_left_color"
                android:textSize="@dimen/title_left_size" />
        </RelativeLayout>


        <TextView
            android:id="@+id/textView3"
            android:layout_width="match_parent"
            android:layout_height="0.50dp"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:background="#aaaaaa" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="@string/newparty_title_text"
            android:textColor="#535353"
            android:textSize="15.0sp" />

        <Button
            android:id="@+id/Add_New_Party_OK"
            android:layout_width="65.0dp"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:background="@null"
            android:gravity="center"
            android:text="@string/newparty_title_right"
            android:textColor="@drawable/title_right_color"
            android:textSize="@dimen/title_right_size" 
            android:enabled="true"
            />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/Add_New_Party_title_layout"
        android:layout_centerHorizontal="true"
        android:background="#efefef" >

        <RelativeLayout
            android:id="@+id/relativeLayout1"
            android:layout_width="match_parent"
            android:layout_height="50.0dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true" >

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20.0dp"
                android:layout_marginTop="25.0dp"
                android:text="主要"
                android:textColor="#808080"
                android:textSize="12.0sp" />

            <TextView
                android:id="@+id/textView2"
                android:layout_width="match_parent"
                android:layout_height="0.50dp"
                android:layout_alignParentBottom="true"
                android:background="#aaaaaa" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/Add_New_Party_message_layout"
            android:layout_width="match_parent"
            android:layout_height="220.0dp"
            android:layout_below="@+id/relativeLayout1"
            android:layout_centerHorizontal="true" >

            <RelativeLayout
                android:id="@+id/relativeLayout2"
                android:layout_width="match_parent"
                android:layout_height="44.0dp"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:background="#ffffff" >

                <EditText
                    android:id="@+id/Add_New_Party_name"
                    android:layout_width="match_parent"
                    android:layout_height="35.0dp"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="52.0dp"
                    android:layout_marginRight="20.0dp"
                    android:background="@null"
                    android:ems="10"
                    android:gravity="left|center"
                    android:hint="聚会名称"
                    android:textColor="#000000"
                    android:textColorHint="#c9c9c9"
                    android:textSize="13.0sp" >

                    <requestFocus android:layout_width="wrap_content" />
                </EditText>

                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="match_parent"
                    android:layout_height="0.50dp"
                    android:layout_alignParentBottom="true"
                    android:layout_marginLeft="20.0dp"
                    android:background="#aaaaaa" />

                <ImageView
                    android:id="@+id/imageView3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="20.0dp"
                    android:src="@drawable/partyname" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/Add_New_Party_time"
                android:layout_width="match_parent"
                android:layout_height="44.0dp"
                android:layout_below="@+id/relativeLayout2"
                android:layout_centerHorizontal="true"
                android:background="#ffffff" >

                <TextView
                    android:id="@+id/textView7"
                    android:layout_width="110.0dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="20.0dp"
                    android:drawableLeft="@drawable/time"
                    android:gravity="right|center"
                    android:text="活动开始时间"
                    android:textSize="13.0sp" />

                <TextView
                    android:id="@+id/textView8"
                    android:layout_width="match_parent"
                    android:layout_height="0.50dp"
                    android:layout_alignParentBottom="true"
                    android:layout_marginLeft="20.0dp"
                    android:background="#aaaaaa" />

                <ImageView
                    android:id="@+id/imageView1"
                    android:layout_width="17.0dp"
                    android:layout_height="17.0dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="20dp"
                    android:src="@drawable/jiantou" />

                <TextView
                    android:id="@+id/Add_New_Party_time_details"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/imageView1"
                    android:layout_alignTop="@+id/imageView1"
                    android:layout_marginLeft="5.0dp"
                    android:layout_marginRight="5.0dp"
                    android:layout_toLeftOf="@+id/imageView1"
                    android:layout_toRightOf="@+id/textView7"
                    android:gravity="right|center"
                    android:singleLine="true"
                    android:text="开始时间"
                    android:textColor="#c9c9c9"
                    android:textSize="12.0sp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/Add_New_Party_address"
                android:layout_width="match_parent"
                android:layout_height="44.0dp"
                android:layout_below="@+id/Add_New_Party_time"
                android:layout_centerHorizontal="true"
                android:background="#ffffff" >

                <TextView
                    android:id="@+id/textView10"
                    android:layout_width="83.0dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="20.0dp"
                    android:drawableLeft="@drawable/map"
                    android:gravity="right|center"
                    android:text="活动地点"
                    android:textSize="13.0dp" />

                <TextView
                    android:id="@+id/textView11"
                    android:layout_width="match_parent"
                    android:layout_height="0.50dp"
                    android:layout_alignParentBottom="true"
                    android:layout_marginLeft="20.0dp"
                    android:background="#aaaaaa" />

                <ImageView
                    android:id="@+id/imageView2"
                    android:layout_width="17.0dp"
                    android:layout_height="17.0dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="20.0dp"
                    android:src="@drawable/jiantou" />

                <TextView
                    android:id="@+id/Add_New_Party_address_details"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/imageView2"
                    android:layout_alignTop="@+id/textView10"
                    android:layout_marginLeft="5.0dp"
                    android:layout_marginRight="5.0dp"
                    android:layout_toLeftOf="@+id/imageView2"
                    android:layout_toRightOf="@+id/textView10"
                    android:gravity="right|center"
                    android:singleLine="true"
                    android:text="地址详情"
                    android:textColor="#c9c9c9"
                    android:textSize="12.0sp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/Add_New_Party_activity_cost_layout"
                android:layout_width="match_parent"
                android:layout_height="44.0dp"
                android:layout_below="@+id/Add_New_Party_address"
                android:layout_centerHorizontal="true"
                android:background="#ffffff" >

                <TextView
                    android:id="@+id/textView12"
                    android:layout_width="83.0dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="20.0dp"
                    android:drawableLeft="@drawable/settingbalance"
                    android:gravity="right|center"
                    android:text="活动花费"
                    android:textSize="13.0sp" />

                <ImageView
                    android:id="@+id/imageView1"
                    android:layout_width="17.0dp"
                    android:layout_height="17.0dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="20dp"
                    android:src="@drawable/jiantou" />

                <TextView
                    android:id="@+id/Add_New_Party_activity_cost"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@+id/textView12"
                    android:layout_marginLeft="5.0dp"
                    android:layout_marginRight="5.0dp"
                    android:layout_toLeftOf="@+id/imageView1"
                    android:layout_toRightOf="@+id/textView12"
                    android:gravity="right|center"
                    android:text="付款方式"
                    android:textColor="#c9c9c9"
                    android:textSize="12.0sp" />

                <TextView
                    android:id="@+id/textView14"
                    android:layout_width="match_parent"
                    android:layout_height="0.50dp"
                    android:layout_alignParentBottom="true"
                    android:layout_marginLeft="20.0dp"
                    android:background="#aaaaaa" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/Add_New_Party_details_layout"
                android:layout_width="match_parent"
                android:layout_height="44.0dp"
                android:layout_below="@+id/Add_New_Party_activity_cost_layout"
                android:layout_centerHorizontal="true"
                android:background="#ffffff" >

                <TextView
                    android:id="@+id/Add_New_Party_details_prompt"
                    android:layout_width="110.0dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="20.0dp"
                    android:drawableLeft="@drawable/chatname"
                    android:gravity="right|center"
                    android:text="聚会详情编辑"
                    android:textSize="13.0sp" />

                <ImageView
                    android:id="@+id/imageView1"
                    android:layout_width="17.0dp"
                    android:layout_height="17.0dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="20dp"
                    android:src="@drawable/jiantou" />

                <TextView
                    android:id="@+id/Add_New_Party_details"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/imageView1"
                    android:layout_marginLeft="5.0dp"
                    android:layout_marginRight="5.0dp"
                    android:layout_toLeftOf="@+id/imageView1"
                    android:layout_toRightOf="@+id/Add_New_Party_details_prompt"
                    android:gravity="right|center"
                    android:text="内容"
                    android:textColor="#c9c9c9"
                    android:textSize="12.0sp" />

                <TextView
                    android:id="@+id/textView26"
                    android:layout_width="match_parent"
                    android:layout_height="0.50dp"
                    android:layout_alignParentBottom="true"
                    android:background="#aaaaaa" />
            </RelativeLayout>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="270.0dp"
            android:layout_below="@+id/Add_New_Party_message_layout"
            android:layout_centerHorizontal="true" >

            <RelativeLayout
                android:id="@+id/relativeLayout3"
                android:layout_width="match_parent"
                android:layout_height="50.0dp"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true" >

                <TextView
                    android:id="@+id/textView1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20.0dp"
                    android:layout_marginTop="25.0dp"
                    android:text="详细"
                    android:textColor="#808080"
                    android:textSize="12.0sp" />

                <TextView
                    android:id="@+id/textView9"
                    android:layout_width="match_parent"
                    android:layout_height="0.50dp"
                    android:layout_alignParentBottom="true"
                    android:background="#aaaaaa" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/Add_New_Party_end_time_layout"
                android:layout_width="match_parent"
                android:layout_height="44.0dp"
                android:layout_below="@+id/relativeLayout3"
                android:layout_centerHorizontal="true"
                android:background="#ffffff" >

                <TextView
                    android:id="@+id/Add_New_Party_end_time_prompt"
                    android:layout_width="110.0dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="20.0dp"
                    android:drawableLeft="@drawable/time"
                    android:gravity="right|center"
                    android:text="活动结束时间"
                    android:textSize="13.0sp" />

                <ImageView
                    android:id="@+id/imageView1"
                    android:layout_width="17.0dp"
                    android:layout_height="17.0dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="20dp"
                    android:src="@drawable/jiantou" />

                <TextView
                    android:id="@+id/textView17"
                    android:layout_width="match_parent"
                    android:layout_height="0.50dp"
                    android:layout_alignParentBottom="true"
                    android:layout_marginLeft="20.0dp"
                    android:background="#aaaaaa" />

                <TextView
                    android:id="@+id/Add_New_Party_end_time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/imageView1"
                    android:layout_marginLeft="5.0dp"
                    android:layout_marginRight="5.0dp"
                    android:layout_toLeftOf="@+id/imageView1"
                    android:layout_toRightOf="@+id/Add_New_Party_end_time_prompt"
                    android:gravity="right|center"
                    android:text="结束时间"
                    android:textColor="#c9c9c9"
                    android:textSize="12.0sp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/Add_New_Party_registration_deadline_layout"
                android:layout_width="match_parent"
                android:layout_height="44.0dp"
                android:layout_below="@+id/Add_New_Party_end_time_layout"
                android:layout_centerHorizontal="true"
                android:background="#ffffff" >

                <TextView
                    android:id="@+id/Add_New_Party_registration_deadline_prompt"
                    android:layout_width="110.0dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="20.0dp"
                    android:drawableLeft="@drawable/time"
                    android:gravity="right|center"
                    android:text="报名截止时间"
                    android:textSize="13.0sp" />

                <ImageView
                    android:id="@+id/imageView1"
                    android:layout_width="17.0dp"
                    android:layout_height="17.0dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="20dp"
                    android:src="@drawable/jiantou" />

                <TextView
                    android:id="@+id/textView20"
                    android:layout_width="match_parent"
                    android:layout_height="0.50dp"
                    android:layout_alignParentBottom="true"
                    android:layout_marginLeft="20.0dp"
                    android:background="#aaaaaa" />

                <TextView
                    android:id="@+id/Add_New_Party_registration_deadline"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/imageView1"
                    android:layout_marginLeft="5.0dp"
                    android:layout_marginRight="5.0dp"
                    android:layout_toLeftOf="@+id/imageView1"
                    android:layout_toRightOf="@+id/Add_New_Party_registration_deadline_prompt"
                    android:gravity="right|center"
                    android:text="截止时间"
                    android:textColor="#c9c9c9"
                    android:textSize="12.0sp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/Add_New_Party_limit_number_layout"
                android:layout_width="match_parent"
                android:layout_height="44.0dp"
                android:layout_below="@+id/Add_New_Party_registration_deadline_layout"
                android:layout_centerHorizontal="true"
                android:background="#ffffff" >

                <TextView
                    android:id="@+id/Add_New_Party_limit_number_prompt"
                    android:layout_width="83.0dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="20.0dp"
                    android:drawableLeft="@drawable/addmember"
                    android:gravity="right|center"
                    android:text="限制人数"
                    android:textSize="13.0sp" />

                <ImageView
                    android:id="@+id/imageView1"
                    android:layout_width="17.0dp"
                    android:layout_height="17.0dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="20dp"
                    android:src="@drawable/jiantou" />

                <TextView
                    android:id="@+id/Add_New_Party_limit_number"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/imageView1"
                    android:layout_marginLeft="5.0dp"
                    android:layout_marginRight="5.0dp"
                    android:layout_toLeftOf="@+id/imageView1"
                    android:layout_toRightOf="@+id/Add_New_Party_limit_number_prompt"
                    android:gravity="right|center"
                    android:text="人数"
                    android:textColor="#c9c9c9"
                    android:textSize="12.0sp" />

                <TextView
                    android:id="@+id/textView23"
                    android:layout_width="match_parent"
                    android:layout_height="0.50dp"
                    android:layout_alignParentBottom="true"
                    android:background="#aaaaaa" />
            </RelativeLayout>
        </RelativeLayout>
    </RelativeLayout>

</RelativeLayout>