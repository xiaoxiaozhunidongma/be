<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#efefef"
    tools:context=".TeamSetting2Activity" >

    <RelativeLayout
        android:id="@+id/TeamSetting2_title_layout"
        android:layout_width="match_parent"
        android:layout_height="@dimen/title_height"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/title_background" >

        <TextView
            android:id="@+id/TeamSetting2_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="@string/setting2_title_text"
            android:textSize="@dimen/title_size" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="match_parent"
            android:layout_height="0.50dp"
            android:layout_alignParentBottom="true"
            android:background="#aaaaaa" />

        <RelativeLayout
            android:id="@+id/TeamSetting2_back_layout"
            android:layout_width="@dimen/title_left_width"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true" >

            <TextView
                android:id="@+id/TeamSetting2_back"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:text="@string/setting2_title_left"
                android:textColor="@drawable/title_left_color"
                android:textSize="@dimen/title_left_size" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/TeamSetting2_save_layout"
            android:layout_width="@dimen/title_right_width"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true" >

            <TextView
                android:id="@+id/TeamSetting2_save"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:text="@string/setting2_title_right"
                android:textColor="@drawable/title_right_color"
                android:textSize="@dimen/title_right_size" />
        </RelativeLayout>
    </RelativeLayout>

    <ScrollView
        android:id="@+id/scrollView1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/TeamSetting2_title_layout"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true" >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/background" >

            <RelativeLayout
                android:id="@+id/TeamSetting2_head_layout"
                android:layout_width="match_parent"
                android:layout_height="180.0dp"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true" >

                <ImageView
                    android:id="@+id/TeamSetting2_head"
                    android:layout_width="90.0dp"
                    android:layout_height="90.0dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="30.0dp"
                    android:contentDescription="@string/app_name"
                    android:scaleType="fitXY"
                    android:src="@drawable/preview_2" />

                <TextView
                    android:id="@+id/TeamSetting2_User_nickname"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/TeamSetting2_head"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="15dp"
                    android:gravity="center"
                    android:text="NAME"
                    android:textColor="#535353"
                    android:textSize="15.0sp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/Setting_User_layout"
                android:layout_width="match_parent"
                android:layout_height="182.0dp"
                android:layout_below="@+id/TeamSetting2_head_layout"
                android:layout_centerHorizontal="true" >

                <RelativeLayout
                    android:id="@+id/relativeLayout3"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/setting_prompt_layout_height"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true" >

                    <TextView
                        android:id="@+id/textView4"
                        android:layout_width="match_parent"
                        android:layout_height="0.50dp"
                        android:layout_alignParentBottom="true"
                        android:background="#aaaaaa" />

                    <TextView
                        android:id="@+id/textView3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="25.0dp"
                        android:text="小组"
                        android:textColor="@drawable/setting_prompt_color"
                        android:textSize="@dimen/setting_size" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/TeamSetting2_Tean_name_layout"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/setting_layout_height"
                    android:layout_below="@+id/relativeLayout3"
                    android:layout_centerHorizontal="true"
                    android:background="@drawable/setting_click_selector" >

                    <TextView
                        android:id="@+id/textView5"
                        android:layout_width="83.0dp"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="20.0dp"
                        android:text="小组名称"
                        android:gravity="right|center"
                        android:drawableLeft="@drawable/groupname"
                        android:textSize="@dimen/setting_size" />

                    <TextView
                        android:id="@+id/textView6"
                        android:layout_width="match_parent"
                        android:layout_height="0.50dp"
                        android:layout_alignParentBottom="true"
                        android:layout_marginLeft="20.0dp"
                        android:background="#aaaaaa" />

                    <ImageView
                        android:id="@+id/Setting_next_1"
                        android:layout_width="15.0dp"
                        android:layout_height="15.0dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="15dp"
                        android:contentDescription="@string/app_name"
                        android:src="@drawable/jiantou" />

                    <TextView
                        android:id="@+id/TeamSetting2_Tean_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignBottom="@+id/Setting_next_1"
                        android:layout_alignTop="@+id/Setting_next_1"
                        android:layout_marginRight="5.0dp"
                        android:layout_toLeftOf="@+id/Setting_next_1"
                        android:gravity="right|center"
                        android:text="名称"
                        android:textColor="@drawable/setting_content_color"
                        android:textSize="@dimen/setting_prompt_size" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/TeamSetting2_invite_friends_layout"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/setting_layout_height"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentLeft="true"
                    android:background="@drawable/setting_click_selector" >

                    <TextView
                        android:id="@+id/textView9"
                        android:layout_width="110.0dp"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="20dp"
                        android:text="邀请好友加入"
                        android:gravity="right|center"
                        android:drawableLeft="@drawable/addfriends"
                        android:textSize="@dimen/setting_size" />

                    <TextView
                        android:id="@+id/textView10"
                        android:layout_width="match_parent"
                        android:layout_height="0.50dp"
                        android:layout_alignParentBottom="true"
                        android:background="#aaaaaa" />

                    <ImageView
                        android:id="@+id/Setting_next_3"
                        android:layout_width="15.0dp"
                        android:layout_height="15.0dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="15dp"
                        android:contentDescription="@string/app_name"
                        android:src="@drawable/jiantou" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/TeamSetting2_History_party_layout"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/setting_layout_height"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/TeamSetting2_Tean_name_layout"
                    android:background="@drawable/setting_click_selector" >

                    <TextView
                        android:id="@+id/textView7"
                        android:layout_width="83.0dp"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="20dp"
                        android:text="历史聚会"
                        android:gravity="right|center"
                        android:drawableLeft="@drawable/oldparty"
                        android:textSize="@dimen/setting_size" />

                    <TextView
                        android:id="@+id/textView8"
                        android:layout_width="match_parent"
                        android:layout_height="0.50dp"
                        android:layout_alignParentBottom="true"
                        android:layout_marginLeft="20.0dp"
                        android:background="#aaaaaa" />

                    <ImageView
                        android:id="@+id/Setting_next_2"
                        android:layout_width="15.0dp"
                        android:layout_height="15.0dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="15dp"
                        android:contentDescription="@string/app_name"
                        android:src="@drawable/jiantou" />
                </RelativeLayout>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/relativeLayout7"
                android:layout_width="match_parent"
                android:layout_height="182.0dp"
                android:layout_below="@+id/Setting_User_layout"
                android:layout_centerHorizontal="true" >

                <RelativeLayout
                    android:id="@+id/relativeLayout6"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/setting_prompt_layout_height"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true" >

                    <TextView
                        android:id="@+id/textView13"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_marginLeft="20.0dp"
                        android:layout_marginTop="25.0dp"
                        android:text="提示"
                        android:textColor="@drawable/setting_prompt_color"
                        android:textSize="@dimen/setting_size" />

                    <TextView
                        android:id="@+id/textView14"
                        android:layout_width="match_parent"
                        android:layout_height="0.50dp"
                        android:layout_alignParentBottom="true"
                        android:layout_centerHorizontal="true"
                        android:background="#aaaaaa" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/TeamSetting2_Message_prompt"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/setting_layout_height"
                    android:layout_below="@+id/relativeLayout6"
                    android:layout_centerHorizontal="true"
                    android:background="@drawable/setting_click_selector" >

                    <TextView
                        android:id="@+id/textView15"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="20.0dp"
                        android:text="聚会信息提示"
                        android:textSize="@dimen/setting_size" />

                    <TextView
                        android:id="@+id/textView16"
                        android:layout_width="match_parent"
                        android:layout_height="0.50dp"
                        android:layout_alignParentBottom="true"
                        android:layout_marginLeft="20.0dp"
                        android:background="#aaaaaa" />

                    <RelativeLayout
                        android:layout_width="50.0dp"
                        android:layout_height="30.0dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="20dp"
                        android:layout_marginTop="10.0dp" >

                        <com.biju.switchutils.MessageSwitchView
                            android:id="@+id/Message_SwitchView1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentTop="true" >
                        </com.biju.switchutils.MessageSwitchView>
                    </RelativeLayout>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/TeamSetting2_chat_prompt"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/setting_layout_height"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/TeamSetting2_Message_prompt"
                    android:background="@drawable/setting_click_selector" >

                    <TextView
                        android:id="@+id/textView17"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="20dp"
                        android:text="聊天信息提示"
                        android:textSize="@dimen/setting_size" />

                    <TextView
                        android:id="@+id/textView18"
                        android:layout_width="match_parent"
                        android:layout_height="0.50dp"
                        android:layout_alignParentBottom="true"
                        android:layout_marginLeft="20.0dp"
                        android:background="#aaaaaa" />

                    <RelativeLayout
                        android:layout_width="50.0dp"
                        android:layout_height="30.0dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="20dp"
                        android:layout_marginTop="10.0dp" >

                        <com.biju.switchutils.ChatSwitchView
                            android:id="@+id/Chat_SwitchView1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentTop="true" >
                        </com.biju.switchutils.ChatSwitchView>
                    </RelativeLayout>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/TeamSetting2_phone_prompt"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/setting_layout_height"
                    android:layout_below="@+id/TeamSetting2_chat_prompt"
                    android:layout_centerHorizontal="true"
                    android:background="@drawable/setting_click_selector" >

                    <TextView
                        android:id="@+id/textView2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="20dp"
                        android:text="公开手机号码"
                        android:textSize="@dimen/setting_size" />

                    <TextView
                        android:id="@+id/textView11"
                        android:layout_width="match_parent"
                        android:layout_height="0.50dp"
                        android:layout_alignParentBottom="true"
                        android:background="#aaaaaa" />

                    <RelativeLayout
                        android:layout_width="50.0dp"
                        android:layout_height="30.0dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="20dp"
                        android:layout_marginTop="10.0dp" >

                        <com.biju.switchutils.PhoneSwitchView
                            android:id="@+id/Phone_SwitchView1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentTop="true" >
                        </com.biju.switchutils.PhoneSwitchView>
                    </RelativeLayout>
                </RelativeLayout>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/relativeLayout11"
                android:layout_width="match_parent"
                android:layout_height="94.0dp"
                android:layout_below="@+id/relativeLayout7"
                android:layout_centerHorizontal="true" >

                <RelativeLayout
                    android:id="@+id/relativeLayout10"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/setting_prompt_layout_height"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true" >

                    <TextView
                        android:id="@+id/textView25"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20.0dp"
                        android:layout_marginTop="25.0dp"
                        android:text="其他"
                        android:textColor="@drawable/setting_prompt_color"
                        android:textSize="@dimen/setting_size" />

                    <TextView
                        android:id="@+id/textView26"
                        android:layout_width="match_parent"
                        android:layout_height="0.50dp"
                        android:layout_alignParentBottom="true"
                        android:background="#aaaaaa" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/TeamSetting2_Exit_layout"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/setting_layout_height"
                    android:layout_below="@+id/relativeLayout10"
                    android:layout_centerHorizontal="true"
                    android:background="@drawable/setting_click_selector" >

                    <TextView
                        android:id="@+id/textView27"
                        android:layout_width="83.0dp"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="20.0dp"
                        android:text="退出小组"
                        android:textColor="#ff0000"
                        android:gravity="right|center"
                        android:drawableLeft="@drawable/settingexit"
                        android:textSize="@dimen/setting_size" />

                    <TextView
                        android:id="@+id/textView28"
                        android:layout_width="match_parent"
                        android:layout_height="0.50dp"
                        android:layout_alignParentBottom="true"
                        android:background="#aaaaaa" />

                    <ImageView
                        android:id="@+id/Setting_next_8"
                        android:layout_width="15.0dp"
                        android:layout_height="15.0dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="15dp"
                        android:contentDescription="@string/app_name"
                        android:src="@drawable/jiantou" />
                </RelativeLayout>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="100.0dp"
                android:layout_below="@+id/relativeLayout11"
                android:layout_centerHorizontal="true" >
            </RelativeLayout>
        </RelativeLayout>
    </ScrollView>

</RelativeLayout>