<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RequestCode3Activity" >

    <RelativeLayout
        android:id="@+id/RequestCode3_title_layout"
        android:layout_width="match_parent"
        android:layout_height="@dimen/title_height"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:background="@drawable/title_background" >

        <RelativeLayout
            android:id="@+id/RequestCode3_back_layout"
            android:layout_width="80.0dp"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true" >

            <TextView
                android:id="@+id/Requestcode3_back"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:drawableLeft="@drawable/left_arrow_2"
                android:gravity="center"
                android:text="@string/request_code_title_left"
                android:textColor="@drawable/title_right_color"
                android:textSize="14.0sp" />
        </RelativeLayout>

        <TextView
            android:id="@+id/textView1"
            android:layout_width="match_parent"
            android:layout_height="0.50dp"
            android:layout_alignParentBottom="true"
            android:background="#aaaaaa" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/Requestcode3_after_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/RequestCode3_title_layout"
        android:layout_centerHorizontal="true" >

        <RelativeLayout
            android:id="@+id/Requestcode3_head_layout"
            android:layout_width="match_parent"
            android:layout_height="150.0dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true" >

            <ImageView
                android:id="@+id/Requestcode3_head"
                android:layout_width="80.0dp"
                android:layout_height="80.0dp"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="20.0dp"
                android:contentDescription="@string/app_name"
                android:scaleType="fitXY"
                android:src="@drawable/preview_2" />

            <TextView
                android:id="@+id/Requestcode3_User_nickname"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Requestcode3_head"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="15dp"
                android:gravity="center"
                android:text="@string/requestcode2_User_nickname"
                android:textSize="15.0sp"
                android:textColor="#535353"
                 />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/Requestcode2_team_data_layout"
            android:layout_width="match_parent"
            android:layout_height="138.0dp"
            android:layout_below="@+id/Requestcode3_head_layout"
            android:layout_centerHorizontal="true" >

            <RelativeLayout
                android:id="@+id/relativeLayout3"
                android:layout_width="match_parent"
                android:layout_height="50.0dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true" >

                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="match_parent"
                    android:layout_height="0.50dp"
                    android:layout_alignParentBottom="true"
                    android:background="#aaaaaa" />

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="25.0dp"
                    android:text="@string/requestcode2_team_data"
                    android:textColor="#808080"
                    android:textSize="13.0sp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/Requestcode3_number"
                android:layout_width="match_parent"
                android:layout_height="44.0dp"
                android:layout_below="@+id/relativeLayout3"
                android:layout_centerHorizontal="true"
                android:background="@drawable/setting_click_selector" >

                <TextView
                    android:id="@+id/textView5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="20.0dp"
                    android:text="@string/requestcode2_number"
                    android:textColor="#000000"
                    android:textSize="13.0sp" />

                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="match_parent"
                    android:layout_height="0.50dp"
                    android:layout_alignParentBottom="true"
                    android:layout_marginLeft="20.0dp"
                    android:background="#aaaaaa" />

                <ImageView
                    android:id="@+id/Requestcode2_next_1"
                    android:layout_width="15.0dp"
                    android:layout_height="15.0dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="15dp"
                    android:contentDescription="@string/app_name"
                    android:src="@drawable/jiantou" />

                <TextView
                    android:id="@+id/Requestcode2_txet"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/Requestcode2_next_1"
                    android:layout_alignTop="@+id/Requestcode2_next_1"
                    android:layout_marginRight="5.0dp"
                    android:layout_toLeftOf="@+id/Requestcode2_next_1"
                    android:gravity="right|center"
                    android:text="@string/requestcode2_number_1"
                    android:textColor="#808080"
                    android:textSize="12.0sp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/Requestcode3_introduce"
                android:layout_width="match_parent"
                android:layout_height="44.0dp"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/Requestcode3_number"
                android:background="@drawable/setting_click_selector" >

                <TextView
                    android:id="@+id/textView7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="20dp"
                    android:text="@string/requestcode2_introduce"
                    android:textColor="#000000"
                    android:textSize="13.0sp" />

                <TextView
                    android:id="@+id/textView8"
                    android:layout_width="match_parent"
                    android:layout_height="0.50dp"
                    android:layout_alignParentBottom="true"
                    android:background="#aaaaaa" />

                <ImageView
                    android:id="@+id/Requestcode3_next_2"
                    android:layout_width="15.0dp"
                    android:layout_height="15.0dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="15dp"
                    android:contentDescription="@string/app_name"
                    android:src="@drawable/jiantou" />
            </RelativeLayout>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/relativeLayout7"
            android:layout_width="match_parent"
            android:layout_height="94.0dp"
            android:layout_below="@+id/Requestcode2_team_data_layout"
            android:layout_centerHorizontal="true" >

            <RelativeLayout
                android:id="@+id/relativeLayout6"
                android:layout_width="match_parent"
                android:layout_height="50.0dp"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true" >

                <TextView
                    android:id="@+id/textView13"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="20.0dp"
                    android:layout_marginTop="25.0dp"
                    android:text="@string/requestcode2_personal"
                    android:textColor="#808080"
                    android:textSize="13.0sp" />

                <TextView
                    android:id="@+id/textView14"
                    android:layout_width="match_parent"
                    android:layout_height="0.50dp"
                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true"
                    android:background="#aaaaaa" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/Requestcode3_public_phone"
                android:layout_width="match_parent"
                android:layout_height="44.0dp"
                android:layout_below="@+id/relativeLayout6"
                android:layout_centerHorizontal="true"
                android:background="@drawable/setting_click_selector" >

                <TextView
                    android:id="@+id/textView15"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="20.0dp"
                    android:text="@string/requestcode2_public_phone"
                    android:textColor="#000000"
                    android:textSize="13.0sp" />

                <TextView
                    android:id="@+id/textView16"
                    android:layout_width="match_parent"
                    android:layout_height="0.50dp"
                    android:layout_alignParentBottom="true"
                    android:background="#aaaaaa" />

                <ImageView
                    android:id="@+id/requestcode2_public_choose"
                    android:layout_width="13.0dp"
                    android:layout_height="13.0dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="20.0dp"
                    android:src="@drawable/gou_3" 
                    android:visibility="gone"
                    />

            </RelativeLayout>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/relativeLayout11"
            android:layout_width="match_parent"
            android:layout_height="94.0dp"
            android:layout_below="@+id/relativeLayout7"
            android:layout_centerHorizontal="true" >

            <RelativeLayout
                android:id="@+id/relativeLayout10"
                android:layout_width="match_parent"
                android:layout_height="50.0dp"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true" >

                <TextView
                    android:id="@+id/textView25"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20.0dp"
                    android:layout_marginTop="25.0dp"
                    android:text="@string/requestcode2_operation"
                    android:textColor="#808080"
                    android:textSize="13.0sp" />

                <TextView
                    android:id="@+id/textView26"
                    android:layout_width="match_parent"
                    android:layout_height="0.50dp"
                    android:layout_alignParentBottom="true"
                    android:background="#aaaaaa" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/Requestcode3_add_team"
                android:layout_width="match_parent"
                android:layout_height="44.0dp"
                android:layout_below="@+id/relativeLayout10"
                android:layout_centerHorizontal="true"
                android:background="@drawable/setting_click_selector" >

                <TextView
                    android:id="@+id/textView27"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="20.0dp"
                    android:text="@string/requestcode2_add_team"
                    android:textColor="#000000"
                    android:textSize="13.0sp" />

                <TextView
                    android:id="@+id/textView28"
                    android:layout_width="match_parent"
                    android:layout_height="0.50dp"
                    android:layout_alignParentBottom="true"
                    android:background="#aaaaaa" />
            </RelativeLayout>
        </RelativeLayout>
    </RelativeLayout>

</RelativeLayout>